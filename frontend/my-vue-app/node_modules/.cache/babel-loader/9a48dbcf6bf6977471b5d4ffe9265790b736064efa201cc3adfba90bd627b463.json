     // 调用接口\n      this.fetchDevicePerfInfo();\n    },\n    methods: {\n      fetchDevicePerfInfo() {\n        const payload = {\n          device_name: this.deviceData.device_name,\n          other_field: this.deviceData.other_field\n        };\n  \n        axios.post('http://127.0.0.1:5000/view_device_perf_info', payload)\n          .then(response => {\n            this.apiResponse = response.data;\n          })\n          .catch(error => {\n            console.error(\"Error fetching device performance info:\", error);\n          });\n      }\n    }\n  };\n  </script>\n  \n  <style scoped>\n  h1 {\n    color: #42b983;\n  }\n  </style>\n  "],"mappings":"AAoBE,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,IAAI;MAChBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACF,UAAS,GAAI;MAChBG,WAAW,EAAE,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,WAAW;MAC1CG,WAAW,EAAE,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC;IACjC,CAAC;;IAED;IACA,IAAI,CAACC,mBAAmB,CAAC,CAAC;EAC5B,CAAC;EACDC,OAAO,EAAE;IACPD,mBAAmBA,CAAA,EAAG;MACpB,MAAME,OAAM,GAAI;QACdN,WAAW,EAAE,IAAI,CAACH,UAAU,CAACG,WAAW;QACxCG,WAAW,EAAE,IAAI,CAACN,UAAU,CAACM;MAC/B,CAAC;MAEDT,KAAK,CAACa,IAAI,CAAC,6CAA6C,EAAED,OAAO,EAC9DE,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACX,WAAU,GAAIW,QAAQ,CAACb,IAAI;MAClC,CAAC,EACAc,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,yCAAyC,EAAEA,KAAK,CAAC;MACjE,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}